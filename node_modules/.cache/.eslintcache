             amount += item.amount;\n                total +=item.amount * item.price;\n\n            })\n            state.amount = amount;\n            state.total= total;\n\n        },\n    },\n    extraReducers: {\n        [getCartItems.pending]: (state) => {\n          state.isLoading = true;\n        },\n        [getCartItems.fulfilled]: (state, action) => {\n          console.log(action);\n          state.isLoading = false;\n          state.cartItems = action.payload;\n        },\n        [getCartItems.rejected]: (state) => {\n          state.isLoading = false;\n        },\n      },\n});\n//console.log(cartSlice);\nexport const{clearCart,removeItem, increase, decrease,calculateTotals}= cartSlice.actions;\nexport default cartSlice.reducer;","/Users/shankarpandey/Desktop/redux-toolkit/src/components/Navbar.js",[],[],["67","68"],"/Users/shankarpandey/Desktop/redux-toolkit/src/icons.js",[],[],"/Users/shankarpandey/Desktop/redux-toolkit/src/cartItems.js",[],[],"/Users/shankarpandey/Desktop/redux-toolkit/src/components/CartContainer.js",[],[],"/Users/shankarpandey/Desktop/redux-toolkit/src/components/CartItem.js",[],[],["69","70"],"/Users/shankarpandey/Desktop/redux-toolkit/src/components/Modal.js",[],[],"/Users/shankarpandey/Desktop/redux-toolkit/src/features/modal/modalSlice.js",[],[],{"ruleId":"71","severity":1,"message":"72","line":15,"column":3,"nodeType":"73","endLine":15,"endColumn":14,"suggestions":"74"},{"ruleId":"71","severity":1,"message":"72","line":19,"column":3,"nodeType":"73","endLine":19,"endColumn":5,"suggestions":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","severity":1,"message":"81","line":2,"column":8,"nodeType":"82","messageId":"83","endLine":2,"endColumn":17},{"ruleId":"76","replacedBy":"84"},{"ruleId":"78","replacedBy":"85"},{"ruleId":"76","replacedBy":"86"},{"ruleId":"78","replacedBy":"87"},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'dispatch'. Either include it 